    <main aria-label="Content">
        <div class="uk-section uk-section-large">
            <div class="uk-container ">
                <div class="uk-card uk-card-default uk-card-body uk-width-1-3@m uk-align-center">
                    <h4 class="section-title">Login with</h4>
                    <form action="/users/signin" method="POST">
                     {{#if msg}}
                    <div class="uk-alert-success" uk-alert>{{msg}}
                    </div>
                    {{/if}}
                           <div  style="margin-left: 35%;">
                                <a href="#" class="uk-icon-button  uk-margin-small-right" uk-icon="facebook"></a>
                                <a href="/users/google" class="uk-icon-button" uk-icon="google-plus"></a>
                           </div>
                            <hr class="uk-divider-icon">
                            <div style="margin-left: 25px;">
                                <div class="uk-margin">
                                    <div class="uk-inline">
                                        <span class="uk-form-icon" uk-icon="icon: user"></span>
                                        <input class="uk-input" type="text" placeholder="email" name="email">
                                    </div>
                                </div>

                                <div class="uk-margin">
                                    <div class="uk-inline">
                                        <span class="uk-form-icon" uk-icon="icon: lock"></span>
                                        <input class="uk-input" type="password" placeholder="Password" name="password">
                                    </div>
                                </div>
                                <button type="submit" class="uk-button uk-margin-small-bottom" style="width: 93%; color: white; background-color: #75b7ff;">Sign in</button>
                                <p class="uk-text-center" style="color:grey;">Looking to create <a href="/users/signup"> an account</a> ?</p>
                            </div>
                    </form>
                </div>
            </div>
        </div>
    </main>

